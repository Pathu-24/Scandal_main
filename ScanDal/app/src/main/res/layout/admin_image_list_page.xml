<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/buttonBackToHomepage"
        android:layout_width="@dimen/_89pxh"
        android:layout_height="@dimen/_90pxv"
        android:layout_gravity="bottom|start"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageLogo"
            android:layout_width="@dimen/_89pxh"
            android:layout_height="@dimen/_90pxh"
            android:layout_gravity="start"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/img_result1"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_result1" />

    </FrameLayout>

    <ListView
        android:id="@+id/listView_ImageListPage"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonBackToHomepage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtImageList" />

    <ImageView
        android:id="@+id/imageUndrawcomplete"
        android:layout_width="148dp"
        android:layout_height="196dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/img_undrawdoneche"
        tools:ignore="ContentDescription"
        tools:src="@drawable/img_undrawdoneche" />

    <TextView
        android:id="@+id/txtImageList"
        style="@style/txtPoetsenOneregular26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="25dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="Image List"
        app:layout_constraintBottom_toTopOf="@+id/listView_ImageListPage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearRowarrowleft"
        tools:text="Image List" />

    <LinearLayout
        android:id="@+id/linearRowarrowleft"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout
            android:id="@+id/buttonBack_ImageListPage"
            style="@style/groupStyleteal_A200cornerRadius"
            android:layout_width="@dimen/_82pxh"
            android:layout_height="@dimen/_51pxv">

            <ImageView
                android:id="@+id/imageArrowleft"
                android:layout_width="@dimen/_23pxh"
                android:layout_height="@dimen/_25pxh"
                android:layout_gravity="top|start"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_arrowleft" />
        </FrameLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/textClickToDelete"
        style="@style/txtPoppinsbold16"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_marginBottom="24dp"
        android:ellipsize="end"
        android:gravity="center_horizontal"
        android:singleLine="true"
        android:text="Click on Image to delete"
        android:textColor="@color/purple_700"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listView_ImageListPage"
        tools:text="Click on Image to delete" />

</androidx.constraintlayout.widget.ConstraintLayout>
